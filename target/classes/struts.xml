<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="default" extends="struts-default, json-default">
        <action name="createLike" class="kr.co.hf.service.LikeCreateAction">
			<result name="CREATE" type="json"></result>
		</action>
		<action name="deleteLike" class="kr.co.hf.service.LikeDeleteAction">
			<result name="DELETE" type="json"></result>
		</action>
		<action name="reportLike" class="kr.co.hf.service.LikeReportAction">
			<result name="REPORT" type="json"></result>
		</action>
    </package>
</struts>